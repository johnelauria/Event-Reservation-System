<h1>Listing organizers</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Crypted password</th>
    <th>Password salt</th>
    <th>Persistence token</th>
    <th>Fname</th>
    <th>Mname</th>
    <th>Lname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @organizers.each do |organizer| %>
  <tr>
    <td><%= organizer.username %></td>
    <td><%= organizer.crypted_password %></td>
    <td><%= organizer.password_salt %></td>
    <td><%= organizer.persistence_token %></td>
    <td><%= organizer.fname %></td>
    <td><%= organizer.mname %></td>
    <td><%= organizer.lname %></td>
    <td><%= link_to 'Show', organizer %></td>
    <td><%= link_to 'Edit', edit_organizer_path(organizer) %></td>
    <td><%= link_to 'Destroy', organizer, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Organizer', new_organizer_path %>
