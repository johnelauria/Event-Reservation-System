<h1>Listing organizers</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>First Name</th>
    <th>Middle Name</th>
    <th>Last Name</th>
    <th>Cellphone Number </th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |organizer| %>
  <tr>
    <td><%= organizer.username %></td>
    <td><%= organizer.email %></td>
    <td><%= organizer.fname %></td>
    <td><%= organizer.minitial %></td>
    <td><%= organizer.lname %></td>
    <td><%= organizer.cellnum %></td>
    <td><%= organizer.address %></td>


    
    <td><%= link_to 'Show', organizer %></td>
    <td><%= link_to 'Edit', edit_user_path(organizer) %></td>
    <td><%= link_to 'Destroy', organizer, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

